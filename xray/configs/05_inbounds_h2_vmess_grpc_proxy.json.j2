{
  {% if hconfigs['vmess_enable'] and hconfigs['grpc_enable'] and hconfigs['h2_enable'] %}
  "inbounds": [
    {
      "tag": "h2-vmess-xhttp-proxy",
      //"listen": "@vmess-grpc-new", // listen on localhost
      "listen": "@@vmess-grpc-proxy",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {% for u in users %}
          {"id": "{{ u['uuid'] }}", "email": "{{ u['uuid'] }}@hiddify.com", "alterId": 0} {% if not loop.last %},{% endif %}
          {% endfor %}
        ]
      },
      "streamSettings": {
        "tcpSettings": {
          "acceptProxyProtocol": true
        },
        "network": "xhttp",
        "security": "none",
        "xhttpSettings": {
          "mode": "stream-up",
          "path": "{{ hconfigs['path_vmess'] }}{{ hconfigs['path_grpc'] }}"
        }
      }
    }
  ]
  {% endif %}
}