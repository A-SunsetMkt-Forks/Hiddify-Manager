{
  {% if hconfigs['vless_enable'] and hconfigs['ws_enable'] %}
  "inbounds": [
    {
      "tag": "vless-xhttp-new",
      "listen": "@@vless-ws-new",
      "protocol": "vless",
      "settings": {
        "clients": [
          {% for u in users %}
          {"id": "{{ u['uuid'] }}", "email": "{{ u['uuid'] }}@hiddify.com"} {% if not loop.last %},{% endif %}
          {% endfor %}
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "xhttp",
        "security": "none",
        "xhttpSettings": {
          "mode": "stream-up",
          "path": "/{{ hconfigs['path_vless'] }}{{ hconfigs['path_ws'] }}"
        },
        "tcpSettings": {
          "acceptProxyProtocol": true
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    }
  ]
  {% endif %}
}