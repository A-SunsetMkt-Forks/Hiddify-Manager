{
	{% if hconfigs['vless_enable'] and hconfigs['httpupgrade_enable'] %}
		"inbounds": [
			{
				"tag":"vless-splithttp",
				"listen": "@@vless-splithttp",
				"protocol": "vless",
				"settings": {
				"clients": [
					{% for u in users %}
						{"id": "{{ u['uuid'] }}", "email": "{{ u['uuid'] }}@hiddify.com"},
					{% endfor %}
				],
				"decryption": "none"
				},
				"streamSettings": {
				"network": "splithttp",
				"security": "none",
				"splithttpSettings": {
					"acceptProxyProtocol": true,
					"path": "/{{ hconfigs['path_vless'] }}{{ hconfigs['path_splithttp'] }}", //Change to your own path
				}
				},
				"sniffing": {
				"enabled": true,
				"destOverride": ["http","tls", "quic"]
				}
			}
		]
	{% endif %}
}